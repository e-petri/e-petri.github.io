<mvc:View
	xmlns="sap.uxap"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:m="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc">
	<m:Page
		showHeader="false">
		<m:landmarkInfo>
			<m:PageAccessibleLandmarkInfo
				rootRole="Region"
				rootLabel="{i18n>SystemSettings_rootLabel}"
				contentRole="Main"
				contentLabel="{i18n>SystemSettings_contentLabel}"/>
		</m:landmarkInfo>
		<ObjectPageLayout>
			<headerTitle>
				<ObjectPageHeader
					objectTitle="Private Projekte"
					objectSubtitle="{i18n>systemSettingsDescriptionTitle}">
				</ObjectPageHeader>
			</headerTitle>
			<headerContent>
			</headerContent>
			<sections>
				<ObjectPageSection>
				<subSections>
					<ObjectPageSubSection
						title="Websites">
						<blocks>
							<ObjectPageLazyLoader>
								
								<l:BlockLayout
										background="Default">
										<l:BlockLayoutRow>
											<l:BlockLayoutCell
												backgroundColorSet="ColorSet9"
												backgroundColorShade="ShadeD">
												<l:VerticalLayout>
													<core:Icon
														src="sap-icon://activities"
														size="2.5rem"
														color="Default"/>
													<m:Title
														text="Websites"
														level="H3"
														titleStyle="H2"
														class="sapUiTinyMarginTopBottom"/>
													<m:Text
														text="{i18n>administrationContent}"
														class="sapUiTinyMarginBottom"/>
												</l:VerticalLayout>
											</l:BlockLayoutCell>
											<l:BlockLayoutCell
												backgroundColorSet="ColorSet9"
												backgroundColorShade="ShadeC"
												width="2">
												<l:VerticalLayout>
													<m:Carousel class="sapUiContentPadding" loop="true">
														<m:pages>
															<m:Image
															src="{
																path: 'view>/isPhone',
																formatter: '.formatter.srcImageValue'
															}"/>
															<m:Image
															src="{
																path: 'view>/isPhone',
																formatter: '.formatter.srcImageValueSecond'
															}"/>
														</m:pages>
													</m:Carousel>
												
												</l:VerticalLayout>
											</l:BlockLayoutCell>
											<l:BlockLayoutCell
												backgroundColorSet="ColorSet9"
												backgroundColorShade="ShadeA">
												<l:VerticalLayout>
													<core:Icon
														src="sap-icon://developer-settings"
														size="2.5rem"
														color="Default"/>
													<m:Title
														text="Entwicklungswerkzeuge"
														level="H3"
														titleStyle="H2"
														class="sapUiTinyMarginTopBottom"/>
													
													<m:Tokenizer  editable="false">
														<m:Token text="SAPUI5"/>
														<m:Token text="JavaScript" />
														<m:Token text="HTML5"/>
														<m:Token text="CSS3" />
													</m:Tokenizer>
													<m:Tokenizer  editable="false">
														<m:Token text="Python"/>
														<m:Token text="C++" />
														<m:Token text="C" />
													</m:Tokenizer>
												</l:VerticalLayout>
											</l:BlockLayoutCell>
										</l:BlockLayoutRow>
									</l:BlockLayout>
							</ObjectPageLazyLoader>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
				</ObjectPageSection>
				<ObjectPageSection>
					<subSections>
						<ObjectPageSubSection
							title="JORO3D-ScanPlus">
							<blocks>
								<ObjectPageLazyLoader>
									<l:BlockLayout
										background="Default">
										<l:BlockLayoutRow>
											<l:BlockLayoutCell
												backgroundColorSet="ColorSet9"
												backgroundColorShade="ShadeA">
												<l:VerticalLayout>
													<core:Icon
														src="sap-icon://activities"
														size="2.5rem"
														color="Default"/>
													<m:Title
														text="Joro3D-SCANplus"
														level="H3"
														titleStyle="H2"
														class="sapUiTinyMarginTopBottom"/>
													<m:Text
														text="{i18n>administrationContent}"
														class="sapUiTinyMarginBottom"/>
												</l:VerticalLayout>
											</l:BlockLayoutCell>
											<l:BlockLayoutCell
												backgroundColorSet="ColorSet9"
												backgroundColorShade="ShadeC"
												width="2">
												<l:VerticalLayout>
													<m:Carousel loop="true">
														<m:pages>
															<m:Image
															src="{
																path: 'view>/isPhone',
																formatter: '.formatter.srcImageValue'
															}"/>
															<m:Image
															src="{
																path: 'view>/isPhone',
																formatter: '.formatter.srcImageValueSecond'
															}"/>
														</m:pages>
													</m:Carousel>
												
												</l:VerticalLayout>
											</l:BlockLayoutCell>
											<l:BlockLayoutCell
												backgroundColorSet="ColorSet9"
												backgroundColorShade="ShadeD">
												<l:VerticalLayout>
													<core:Icon
														src="sap-icon://developer-settings"
														size="2.5rem"
														color="Default"/>
													<m:Title
														text="Entwicklungswerkzeuge"
														level="H3"
														titleStyle="H2"
														class="sapUiTinyMarginTopBottom"/>
													
													<m:Tokenizer  editable="false">
														<m:Token text="SAPUI5"/>
														<m:Token text="JavaScript" />
														<m:Token text="HTML5"/>
														<m:Token text="CSS3" />
													</m:Tokenizer>
													<m:Tokenizer  editable="false">
														<m:Token text="Python"/>
														<m:Token text="C++" />
														<m:Token text="C" />
													</m:Tokenizer>
													
												<!--	<Text SAPUI5, C++, C, Python, HTML5, JavaScript, CSS
														text="{i18n>supplierContent}"
														class="sapUiTinyMarginBottom"/>
													<Link
														text="{i18n>accessLink}"
														tooltip="{i18n>accessSuppliersLink}"/>
													<Link
														text="{i18n>learnLink}"
														tooltip="{i18n>learnSuppliersTooltip}"/>-->
												</l:VerticalLayout>
											</l:BlockLayoutCell>
										</l:BlockLayoutRow>
									</l:BlockLayout>
								</ObjectPageLazyLoader>
							</blocks>
						</ObjectPageSubSection>
					</subSections>
				</ObjectPageSection>
			</sections>
		</ObjectPageLayout>
	</m:Page>
</mvc:View>
