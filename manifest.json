{
	"_version": "1.8.0",
	"sap.app": {
		"id": "sap.ui.demo.toolpageapp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.demo.toolpageapp.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.tnt": {},
				"sap.ui.layout": {},
				"sap.uxap": {}
			}
		},
		"resources": {
		  "css": [
			{
			  "uri": "css/style.css"
			}
		  ]
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.toolpageapp.i18n.i18n"
				}
			},
			"side": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/sideContent.json"
			},
			"alerts": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/alerts.json"
			},
			"customer": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/customers.json"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"controlId": "mainContents",
				"viewType": "XML",
				"controlAggregation": "pages",
				"viewPath": "sap.ui.demo.toolpageapp.view",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": "home"
				},
				{
					"pattern": "MasterSettings",
					"name": "masterSettings",
					"target": [
						"detailSettings",
						"masterSettings"
					]
				},
				{
					"pattern": "MasterPrjPrivate",
					"name": "masterPrjPrivate",
					"target": [
						"detailPrjPrivate",
						"masterPrjPrivate"
					]
				},
				{
					"pattern": "SystemSettings",
					"name": "systemSettings",
					"target": "detailSettings"
				},
				{
					"pattern": "SystemAppPriv",
					"name": "systemAppPriv",
					"target": "detailPrjPrivate"
				},
				{
					"pattern": "SystemAppPrivYourtube",
					"name": "SystemAppPrivYourtube",
					"target": "detailPrjPrivYourtube"
				},
				{
					"pattern": "Statistics",
					"name": "statistics",
					"target": "statistics"
				},
				{
					"pattern": "Legal",
					"name": "legal",
					"target": "legal"
				},
				{
					"pattern": "ProjectsJob",
					"name": "projectsJob",
					"target": "projectsJob"
				},
				{
					"pattern": "ProjectsPrivate",
					"name": "projectsPrivate",
					"target": "projectsPrivate"
				}
			],
			"targets": {
				"home": {
					"viewId": "home",
					"viewName": "Home",
					"routerClass": "sap.m.routing.Router",
					"controlId": "mainContents",
					"viewType": "XML",
					"controlAggregation": "pages",
					"viewPath": "sap.ui.demo.toolpageapp.view",
					"async": true
				},
				"appSettings": {
					"viewId": "appSettings",
					"viewName": "settings.AppSettings",
					"routerClass": "sap.m.routing.Router",
					"controlId": "mainContents",
					"viewType": "XML",
					"controlAggregation": "pages",
					"viewPath": "sap.ui.demo.toolpageapp.view",
					"async": true
				},
				"masterSettings": {
					"viewId": "masterSettings",
					"viewName": "settings.MasterSettings",
					"controlId": "appSettings",
					"controlAggregation": "masterPages",
					"parent": "appSettings",
					"routerClass": "sap.m.routing.Router",
					"viewType": "XML",
					"viewPath": "sap.ui.demo.toolpageapp.view",
					"async": true
				},
				"detailSettings": {
					"viewId": "detailSettings",
					"viewName": "settings.DetailSettings",
					"controlId": "appSettings",
					"controlAggregation": "detailPages",
					"parent": "appSettings",
					"routerClass": "sap.m.routing.Router",
					"viewType": "XML",
					"viewPath": "sap.ui.demo.toolpageapp.view",
					"async": true
				},
				"appPrjPrivate": {
					"viewId": "appPrjPrivate",
					"viewName": "projectsPrivate.AppPrjPrivate",
					"routerClass": "sap.m.routing.Router",
					"controlId": "mainContents",
					"viewType": "XML",
					"controlAggregation": "pages",
					"viewPath": "sap.ui.demo.toolpageapp.view",
					"async": true
				},
				"masterPrjPrivate": {
					"viewId": "masterPrjPrivate",
					"viewName": "projectsPrivate.MasterPrjPrivate",
					"controlId": "appPrjPrivate",
					"controlAggregation": "masterPages",
					"parent": "appPrjPrivate",
					"routerClass": "sap.m.routing.Router",
					"viewType": "XML",
					"viewPath": "sap.ui.demo.toolpageapp.view",
					"async": true
				},
				"detailPrjPrivate": {
					"viewId": "detailPrjPrivate",
					"viewName": "projectsPrivate.DetailPrjPrivate",
					"controlId": "appPrjPrivate",
					"controlAggregation": "detailPages",
					"parent": "appPrjPrivate",
					"routerClass": "sap.m.routing.Router",
					"viewType": "XML",
					"viewPath": "sap.ui.demo.toolpageapp.view",
					"async": true
				},
				"detailPrjPrivYourtube": {
					"viewId": "detailPrjPrivYourtube",
					"viewName": "projectsPrivate.DetailPrjPrivYourtube",
					"controlId": "appPrjPrivate",
					"controlAggregation": "detailPages",
					"parent": "appPrjPrivate",
					"routerClass": "sap.m.routing.Router",
					"viewType": "XML",
					"viewPath": "sap.ui.demo.toolpageapp.view",
					"async": true
				},
				"statistics": {
					"viewId": "statistics",
					"viewName": "Statistics",
					"routerClass": "sap.m.routing.Router",
					"controlId": "mainContents",
					"viewType": "XML",
					"controlAggregation": "pages",
					"viewPath": "sap.ui.demo.toolpageapp.view",
					"async": true
				},
				"legal": {
					"viewId": "legal",
					"viewName": "Legal",
					"routerClass": "sap.m.routing.Router",
					"controlId": "mainContents",
					"viewType": "XML",
					"controlAggregation": "pages",
					"viewPath": "sap.ui.demo.toolpageapp.view",
					"async": true
				},
				"projectsJob": {
					"viewId": "projectsJob",
					"viewName": "ProjectsJob",
					"routerClass": "sap.m.routing.Router",
					"controlId": "mainContents",
					"viewType": "XML",
					"controlAggregation": "pages",
					"viewPath": "sap.ui.demo.toolpageapp.view",
					"async": true
				},
				"projectsPrivate": {
					"viewId": "projectsPrivate",
					"viewName": "ProjectsPrivate",
					"routerClass": "sap.m.routing.Router",
					"controlId": "mainContents",
					"viewType": "XML",
					"controlAggregation": "pages",
					"viewPath": "sap.ui.demo.toolpageapp.view",
					"async": true
				}
			}
		}
	}
}